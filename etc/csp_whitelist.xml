<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="fc-js" type="host">*.feecompass.com</value>
                <value id="fc-js-testenv" type="host">*.fee-compass.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="fc-api" type="host">*.feecompass.com</value>
                <value id="fc-api-testenv" type="host">*.fee-compass.com</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="gstatic" type="host">fonts.gstatic.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="fonts-google" type="host">fonts.googleapis.com</value>
                <value id="fc-styles" type="host">*.feecompass.com</value>
                <value id="fc-styles-testenv" type="host">*.fee-compass.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
